{
  "name": "rk-vscode",
  "displayName": "rk-vscode",
  "description": "A VS Code extension by and for RKeelan",
  "version": "0.2.0",
  "publisher": "rkeelan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rkeelan/rk-vscode"
  },
  "engines": {
    "vscode": "^1.109.0"
  },
  "enabledApiProposals": [
    "terminalDataWriteEvent"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rk-vscode.copyPath",
        "title": "Copy Path"
      },
      {
        "command": "rk-vscode.copyRelativePath",
        "title": "Copy Relative Path"
      },
      {
        "command": "rk-vscode.copyOpenGrokPath",
        "title": "Copy OpenGrok Path"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "rk-vscode.copyPath",
          "group": "9_cutcopypaste@100"
        },
        {
          "command": "rk-vscode.copyRelativePath",
          "group": "9_cutcopypaste@101"
        },
        {
          "command": "rk-vscode.copyOpenGrokPath",
          "group": "9_cutcopypaste@102",
          "when": "resourcePath =~ /^\\/src\\/.*/"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src",
    "test": "npm run compile && mocha out/test/**/*.test.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.2.2",
    "@types/vscode": "^1.109.0",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^9.39.2",
    "mocha": "^11.7.5",
    "typescript": "^5.3.0"
  }
}
