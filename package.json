{
  "name": "rk-vscode",
  "displayName": "rk-vscode",
  "description": "A VS Code extension by and for RKeelan",
  "version": "0.0.1",
  "publisher": "rkeelan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rkeelan/rk-vscode"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rk-vscode.copyPath",
        "title": "Copy Path"
      },
      {
        "command": "rk-vscode.copyRelativePath",
        "title": "Copy Relative Path"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "rk-vscode.copyPath",
          "group": "9_cutcopypaste@100"
        },
        {
          "command": "rk-vscode.copyRelativePath",
          "group": "9_cutcopypaste@101"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "typescript": "^5.3.0"
  }
}
